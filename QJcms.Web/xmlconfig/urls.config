  <rewrite name="message" type="list" page="message.aspx" inherit="QJcms.Web.UI.Page.article_list" templet="message.html" channel="danye">
    <item path="message.aspx" pattern="message.aspx" />
  </rewrite>
  <rewrite name="about" type="list" page="about.aspx" inherit="QJcms.Web.UI.Page.article_list" templet="about.html" channel="danye">
    <item path="about.aspx" pattern="about.aspx" />
  </rewrite>
</urls>